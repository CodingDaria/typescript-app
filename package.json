{
  "name": "typescript-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/eslint-parser": "^7.16.0",
    "@tailwindcss/cli": "^4.0.6",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint-plugin-prettier": "^4.0.0",
    "postcss": "^8.4.47",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.0",
    "sass": "^1.48.0",
    "tailwindcss": "^4.0.6",
    "typescript": "^4.1.2",
    "vite": "^6.1.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "yarn tailwind-build && VITE_CJS_TRACE=true vite",
    "build": "yarn tailwind-build && tsc -b && vite build",
    "preview": "yarn tailwind-build && vite preview",
    "test": "",
    "prettier": "prettier ./src/**/*.{ts,tsx} --write",
    "lint": "eslint -c .eslintrc --ext .js,.jsx,.ts,.tsx src",
    "prepare": "husky install",
    "tailwind-build": "npx @tailwindcss/cli -i src/assets/css/index.scss -o src/assets/css/main.scss"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^20.14.8",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.20",
    "@types/react-router-dom": "^5.3.2",
    "@types/redux-persist": "^4.3.1",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "2.4.1",
    "vite-plugin-svgr": "^4.3.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix --max-warnings=20"
  }
}
